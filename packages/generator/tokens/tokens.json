{
  "theme granite": {
    "mode 1": {
      "primitive": {
        "white": {
          "$type": "color",
          "$description": "",
          "$value": "#ffffff",
          "extensions": {
            "pro.DesignSystemProject": {
              "scopes": [
                "ALL_SCOPES"
              ]
            }
          }
        },
        "blue": {
          "opacity-50": {
            "$type": "color",
            "$description": "",
            "$value": "#360ae980",
            "extensions": {
              "pro.DesignSystemProject": {
                "scopes": [
                  "ALL_SCOPES"
                ]
              }
            }
          }
        }
      },
      "component": {
        "background": {
          "$type": "color",
          "$description": "",
          "$value": "{theme granite.mode 1.primitive.white}",
          "extensions": {
            "pro.DesignSystemProject": {
              "scopes": [
                "ALL_SCOPES"
              ]
            }
          }
        },
        "border": {
          "$type": "color",
          "$description": "",
          "$value": "{theme granite.mode 1.primitive.blue.opacity-50}",
          "extensions": {
            "pro.DesignSystemProject": {
              "scopes": [
                "ALL_SCOPES"
              ]
            }
          }
        }
      }
    }
  },
  "radius": {
    "mode 1": {
      "radius-xs": {
        "$type": "dimension",
        "$value": 4,
        "extensions": {
          "pro.DesignSystemProject": {
            "scopes": [
              "ALL_SCOPES"
            ]
          }
        }
      },
      "radius-xl": {
        "$type": "dimension",
        "$value": 16,
        "extensions": {
          "pro.DesignSystemProject": {
            "scopes": [
              "ALL_SCOPES"
            ]
          }
        }
      },
      "component": {
        "button": {
          "radius": {
            "$type": "dimension",
            "$value": "{radius.mode 1.radius-xs}",
            "extensions": {
              "pro.DesignSystemProject": {
                "scopes": [
                  "CORNER_RADIUS"
                ]
              }
            }
          }
        }
      },
      "border": {
        "$type": "color",
        "$description": "",
        "$value": "{theme granite.2320:0.primitive.white}",
        "extensions": {
          "pro.DesignSystemProject": {
            "scopes": [
              "ALL_SCOPES"
            ]
          }
        }
      }
    }
  },
  "fonts": {
    "mode 1": {
      "$type": "figma-variable-type:STRING"
    }
  },
  "booleans": {
    "mode 1": {
      "$type": "figma-variable-type:BOOLEAN"
    }
  }
}